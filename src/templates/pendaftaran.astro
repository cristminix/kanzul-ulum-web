---
import { contact, company, contact_person } from "../data/site-data.json";
import PendaftaranMainContent from "./sections/pendaftaran-main-content.astro";

const page = {
	title: `Pendaftaran ${company.name} `,
	meta: {
		name: "pendaftaran ponpes kanzul ulum kesambi, pendaftaran pondok pesantren kanzul ulum, pendaftaran pondok pesantren kanzul ulum kesambi cirebon, pondok pesantren kanzul ulum kesambi, pondok pesantren kanzul ulum cirebon, pondok pesantren di cirebon, ponpes cirebon, pengurus pondok pesantren kanzul ulum",
		description: `Pendaftaran ${company.shortAddress} Hub. ${contact.phone}`,
	},
};

import ColumnLayout from "./layouts/column.astro";
const { frontmatter } = Astro.props;
import "../styles/pendaftaran.css";
---

<ColumnLayout page={page} contact={contact} cp={contact_person} company={company}>
  <PendaftaranMainContent company={company}/> 
</ColumnLayout>
 