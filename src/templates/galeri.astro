---
import { contact, company, contact_person } from "../data/site-data.json";
import GaleriMainContent from "./sections/galeri-main-content.astro";

const page = {
	title: `Galeri ${company.name} `,
	meta: {
		name: "galeri ponpes kanzul ulum kesambi, galeri pondok pesantren kanzul ulum, galeri pondok pesantren kanzul ulum kesambi cirebon, pondok pesantren kanzul ulum kesambi, pondok pesantren kanzul ulum cirebon, pondok pesantren di cirebon, ponpes cirebon, pengurus pondok pesantren kanzul ulum",
		description: `Galeri ${company.shortAddress} Hub. ${contact.phone}`,
	},
};
import galery from "../data/galery.json";

import ColumnLayout from "./layouts/column.astro";
const { frontmatter } = Astro.props;
import "../styles/galeri.css";
---

<ColumnLayout page={page} contact={contact} cp={contact_person} company={company}>
  <GaleriMainContent galery={galery}/>
</ColumnLayout>
 