const page = {
	title: `Profil ${company.name} Pengurus ${company.shortAddress}`,
	meta: {
		name: "profil ponpes kanzul ulum kesambi, profil pondok pesantren kanzul ulum, profil pondok pesantren kanzul ulum kesambi cirebon, pondok pesantren kanzul ulum kesambi, pondok pesantren kanzul ulum cirebon, pondok pesantren di cirebon, ponpes cirebon, pengurus pondok pesantren kanzul ulum",
